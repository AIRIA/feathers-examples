<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/3.2">

	<id>org.josht.foxhole.DisplayObjectExplorer</id>

	<filename>Foxhole Display Object Explorer</filename>

	<name>Foxhole Display Object Explorer</name>
	
	<versionNumber>1.0.0</versionNumber>
		         
	<description>Display Object Explorer example application built with Foxhole for Starling</description>

	<copyright>2012 Josh Tynjala</copyright>

	<initialWindow>
		<content>DisplayObjectExplorer.swf</content>
		<aspectRatio>portrait</aspectRatio>
		<autoOrients>true</autoOrients>
		<fullScreen>true</fullScreen>
		<visible>true</visible>
		<renderMode>direct</renderMode>
	</initialWindow>

	<supportedProfiles>mobileDevice</supportedProfiles>

	<!--<icon>
		<image48x48>icon48.png</image48x48>
		<image72x72>icon72.png</image72x72>
		<image128x128>icon128.png</image128x128>
		<image512x512>icon512.png</image512x512>
	</icon>-->
	<android>
		<colorDepth>16bit</colorDepth>
		<manifestAdditions><![CDATA[
			<manifest android:installLocation="auto">
				<uses-sdk android:targetSdkVersion="11"/>
			</manifest>
		]]></manifestAdditions>
	</android>
	<iPhone>
		<InfoAdditions>
		<![CDATA[
			<key>UIDeviceFamily</key> 
			<array> 
				<string>1</string>
				<string>2</string>
			</array>
			<key>UIPrerenderedIcon</key>
			<true/>
		]]> 
		</InfoAdditions>
		<requestedDisplayResolution>high</requestedDisplayResolution>
	</iPhone>
</application>